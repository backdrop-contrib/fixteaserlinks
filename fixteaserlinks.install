<?php

/**
 * @file
 * Install and uninstall hooks.
 */

/**
 * Implements hook_uninstall().
 */
function fixteaserlinks_uninstall() {
  \Drupal::config('fixteaserlinks.settings')->clear('fixteaserlinks_comment')->save();
  \Drupal::config('fixteaserlinks.settings')->clear('fixteaserlinks_readmore')->save();
}
